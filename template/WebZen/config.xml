<?xml version="1.0" encoding="utf-8" ?>
<cu3er>
	<settings>
		<auto_play>
			<defaults symbol="circular" time="5" />
			<tweenIn x="860" y="30" width="35" height="35" tint="0xFFFFFF" />
		</auto_play>
		
    	<prev_button>
			<defaults round_corners="5,5,5,5"/>
			<tweenOver tint="0xFFFFFF" scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</prev_button>
		
    	<prev_symbol>
			<tweenOver tint="0x000000" />			
		</prev_symbol>
		
    	<next_button>
			<defaults round_corners="5,5,5,5"/>			
			<tweenOver tint="0xFFFFFF"  scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</next_button>
		
    	<next_symbol>
			<tweenOver tint="0x000000" />
		</next_symbol>	
			
		
		<description>
		<defaults round_corners="10, 10, 10, 10" heading_text_size="22" heading_text_color="0xffffff" paragraph_text_size="16" paragraph_text_color="0xFFFFFF"/>
		<tweenIn x="25" y="225" width="850" height="70" alpha="0.50"/>
		<tweenOver alpha="0.80"/>
		</description>
			
	</settings>    

	<slides>
	
		<!-- changing transition beetween second & third slide -->
        <transition duration="0.10" delay=".2" direction="down"/>
		
        <slide>
       		<url>images/slide_1.jpg</url>
        </slide>
		
		<!-- changing transition beetween first & second slide -->
		
        <transition num="5"  slicing="horizontal" direction="right" duration="0.8" delay="0.05" z_multiplier="5"/>
		
		<!-- changing transition beetween second & third slide -->
        <transition duration="0.6" delay=".2" direction="down"/>
		
        <slide>
       		<url>images/slide_2.jpg</url>
        </slide>

<transition num="6" slicing="vertical" direction="up" shader="phong" light_position="0, 0, -100" delay="0.05" z_multiplier="4" />
		
		<!-- changing transition beetween second & third slide -->
        <transition duration="0.6" delay=".2" direction="down"/>
		
		<slide>
       		<url>images/slide_3.jpg</url>
        </slide>		
		
		<!-- changing transition beetween second & third slide -->
        <transition duration="0.6" delay=".2" direction="down"/>
		
		<slide>
       		<url>images/slide_4.jpg</url>
        </slide>		
		
		<!-- changing transition beetween second & third slide -->
        <transition duration="0.6" delay=".2" direction="down"/>
		
		<slide>
       		<url>images/slide_5.jpg</url>
        </slide>
		
	</slides>
</cu3er>

